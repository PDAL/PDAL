{
  "Pipeline": {
    "Readers": [{
      "type": "readers.las",
      "options": [{
        "name": "filename",
        "value": "@CMAKE_SOURCE_DIR@/test/data/autzen/autzen-utm.las"
      }]
    }],
    "Filters": [{
      "type": "filters.programmable",
      "options": [{
        "name": "function",
        "value": "myfunc"
      }, {
        "name": "module",
        "value": "derive"
      }, {
        "name": "source",
        "value": "import numpy as np\ndef myfunc(ins,outs):\nX = ins['Y']\nX1 = np.zeros(X.size, dtype=type(X[0])) + 314\nouts['Y'] = X1\nreturn True"
      }]
    }]
  }
}
